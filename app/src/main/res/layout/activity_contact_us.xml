<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
        android:layout_height="fill_parent">
    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.gretchen.agrodata.activities.ContactUs">

        <TextView
            android:id="@+id/CU_ADT_text_Textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="32dp"
            android:layout_marginTop="24dp"
            android:text="TextView"
            android:textSize="30sp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_AEDA_email_TextView"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/CU_AEDA_email_TextView" />

        <TextView
            android:id="@+id/CU_ADT_tel_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/CU_ADT_text_Textview"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_ADT_text_Textview" />

        <TextView
            android:id="@+id/CU_ADT_email_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_ADT_tel_TextView"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/CU_ADT_tel_TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="32dp" />

        <TextView
            android:id="@+id/CU_AEDA_text_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="32dp"
            android:layout_marginTop="24dp"
            android:text="TextView"
            android:textSize="30sp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_FIDA_email_TextView"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/CU_FIDA_email_TextView" />

        <TextView
            android:id="@+id/CU_AEDA_tel_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_AEDA_text_TextView"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/CU_AEDA_text_TextView" />

        <TextView
            android:id="@+id/CU_AEDA_email_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/CU_AEDA_tel_TextView"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_AEDA_tel_TextView" />

        <TextView
            android:id="@+id/CU_FIDA_text_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="32dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textSize="30sp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/CU_FIDA_email_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginRight="112dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_FIDA_tel_TextView"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/CU_FIDA_tel_TextView" />

        <TextView
            android:id="@+id/CU_FIDA_tel_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginLeft="-10dp"
            app:layout_constraintLeft_toLeftOf="@+id/CU_FIDA_text_TextView"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/CU_FIDA_text_TextView" />
    </android.support.constraint.ConstraintLayout>
</ScrollView>