<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gretchen.agrodata.activities.MainPage">

    <Button
        android:id="@+id/MP_log_out_button"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="7dp"
        android:onClick="logOut"
        android:text="Log Out"
        android:visibility="visible"
        app:layout_constraintLeft_toRightOf="@+id/MP_view_profile_button"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/MP_banana_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_marginLeft="16dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/banana"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.003"
        app:layout_constraintTop_toBottomOf="@+id/MP_add_product_button" />

    <ImageButton
        android:id="@+id/MP_cow_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/cow"
        app:layout_constraintTop_toTopOf="@+id/MP_banana_button"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_banana_button"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/MP_bean_button"
        app:layout_constraintRight_toLeftOf="@+id/MP_hay_button"
        android:layout_marginRight="0dp" />

    <ImageButton
        android:id="@+id/MP_chicken_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_marginRight="16dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/chicken"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/MP_cow_button"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/MP_cow_button"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_cow_button"
        android:layout_marginBottom="0dp" />

    <ImageButton
        android:id="@+id/MP_brocolli_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_marginBottom="196dp"
        android:layout_marginLeft="16dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/brocolli"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MP_banana_button" />

    <ImageButton
        android:id="@+id/MP_bean_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/bean"
        app:layout_constraintTop_toTopOf="@+id/MP_brocolli_button"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_brocolli_button"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/MP_pig_button"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/MP_pig_button" />

    <ImageButton
        android:id="@+id/MP_hay_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_marginRight="16dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hay"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/MP_bean_button"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/MP_bean_button"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_bean_button"
        android:layout_marginBottom="0dp" />

    <ImageButton
        android:id="@+id/MP_pig_button"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/pig"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/MP_secret_user_button"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MP_bean_button"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/MP_fruit_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="42dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:text="Fruit"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_banana_button"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/MP_banana_button"
        app:layout_constraintRight_toLeftOf="@+id/MP_cow_button"
        app:layout_constraintTop_toBottomOf="@+id/MP_add_product_button"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/MP_dairy_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dairy"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/MP_chicken_button"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/MP_cow_button"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/MP_cow_button"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_cow_button"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/MP_poultry_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poultry"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="@+id/MP_chicken_button"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/MP_chicken_button"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/MP_chicken_button"
        android:layout_marginTop="27dp"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintBottom_toBottomOf="@+id/MP_chicken_button"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/MP_vegetable_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Vegetables"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_brocolli_button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/MP_brocolli_button"
        app:layout_constraintRight_toLeftOf="@+id/MP_bean_button"
        app:layout_constraintTop_toTopOf="@+id/MP_brocolli_button"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/MP_grain_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="49dp"
        android:text="Grain"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_bean_button"
        app:layout_constraintLeft_toLeftOf="@+id/MP_bean_button"
        app:layout_constraintRight_toLeftOf="@+id/MP_hay_button"
        app:layout_constraintTop_toTopOf="@+id/MP_bean_button"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/MP_hay_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Hay"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_hay_button"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintLeft_toLeftOf="@+id/MP_hay_button"
        app:layout_constraintRight_toRightOf="@+id/MP_hay_button"
        app:layout_constraintTop_toTopOf="@+id/MP_hay_button"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/MP_meat_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meat"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="@+id/MP_pig_button"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/MP_pig_button"
        app:layout_constraintHorizontal_bias="0.511"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="@+id/MP_pig_button"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/MP_pig_button"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/MP_view_profile_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:onClick="viewProfile"
        android:text="View Profile"
        app:layout_constraintLeft_toRightOf="@+id/MP_add_product_button"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/MP_add_product_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:onClick="goToAddProduct"
        android:text="Add Product"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/MP_secret_user_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="checkUsers"
        android:text="View all users"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MP_meat_TextView"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />


</android.support.constraint.ConstraintLayout>
